            <div class="col-12">
                <div class="row mt-5 pt-3">
                    {% for package in packages.data %}
                        <div class="col-lg-3 col-md-6 col-12 mb-5 px-mb-0 px-lg-0">
                        <div class="card card-package {{ package.is_best_seller == false ?: 'bestseller'}}">
                            {% if package.is_best_seller %}
                            <div class="ribbon"><span>Best seller!</span></div>
                            {% endif %}
                            <ul class="list-group list-group-flush">
                                <li class="list-group-item card-package-header">{{ package.title }}</li>
                                <li class="list-group-item card-package-price">
                                    <p class="text-decoration-line-through mb-0"> Rp {{ package.base_price }}</p> 
                                    {% set price = package.price|split('.') %}
                                    <p class="mb-0"> Rp  <b class="text-head1 align-baseline-middle">{{price[0]}}</b><b>.{{price[1] ?? '000'}}</b>/ bln</p>
                                </li>
                                <li class="list-group-item card-package-user"><b>{{ package.user_count }}</b> Pengguna Terdaftar</li>
                            </ul>
                            <div class="card-body text-center card-package-feature">
                                <ul class="list-unstyled">
                                    {% for feature in package.features %}
                                        <li> <b>{{ feature.value }}</b> {{ feature.key }} </li>
                                    {% endfor %}
                                </ul>
                                <a href="{{ package.url }}" class="btn {{ package.is_best_seller == false ? 'btn-outline-dark': 'btn-primary'}} rounded-custom" > {{ package.url_title ?? "Pilih Sekarang" }} </a>
                            </div>
                        </div>
                    </div>
                    {% endfor %}
                </div>
            </div>
        </div>
